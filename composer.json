{
  "name": "2spark/framework-bundle",
  "description": "2Spark Framework Bundle",
  "type": "bundle",
  "authors": [
    {
      "name": "pierretranchard",
      "email": "pierre@tranchard.net"
    }
  ],
  "keywords": [
    "2Spark Framework Bundle",
    "Doctrine",
    "MongoDB",
    "KNP Menu",
    "Param Converter",
    "Validator",
    "Varnish",
    "Logger",
    "Scrambler",
    "One Time Password",
    "Fixtures"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=5.4.37"
  },
  "require-dev": {
    "symfony/symfony": "~2.7|~2.8",
    "doctrine/orm": "~2.2,>=2.2.3,<2.5",
    "doctrine/dbal": "<2.5",
    "doctrine/doctrine-bundle": "~1.4",
    "symfony/assetic-bundle": "~2.3",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "sensio/distribution-bundle": "~3.0,>=3.0.12",
    "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
    "incenteev/composer-parameter-handler": "~2.0",
    "phpunit/phpunit": "^4.7",
    "doctrine/mongodb-odm": "1.0.*@dev",
    "doctrine/mongodb-odm-bundle": "3.0.*@dev",
    "knplabs/knp-paginator-bundle": "^2.4",
    "doctrine/doctrine-fixtures-bundle": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Spark\\FrameworkBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spark\\FrameworkBundle\\Tests\\": "Tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
